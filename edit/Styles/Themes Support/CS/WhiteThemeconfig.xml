      <lexem BeginBlock="double" Type="KeyWord" />
      <lexem BeginBlock="short" Type="KeyWord" />
      <lexem BeginBlock="else" Type="KeyWord" />
      <lexem BeginBlock="sizeof" Type="KeyWord" />
      <lexem BeginBlock="enum" Type="KeyWord" />
